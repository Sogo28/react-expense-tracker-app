{
  "name": "react-expense-tracker-app",
  "version": "1.0.0",
  "description": "Backend server for react expense tracker app ",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node --env-file=.env server",
    "server": "node --env-file=.env --watch server",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Sogo28",
  "license": "MIT",
  "dependencies": {
    "colors": "^1.4.0",
    "express": "^4.19.2",
    "mongodb": "^6.8.0",
    "mongoose": "^8.5.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}